                                                    @Html.LabelFor(model => model.Address, htmlAttributes: new { @class = "control-label col-md-2", style = "color: black;" })
                                                    <div class="col-md-10">
                                                        @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control", style = "color: darkblue;", placeholder = "Your address" } })
                                                        @Html.ValidationMessageFor(model => model.Address, "", new { @class = "text-danger" })
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Cand_Pass, htmlAttributes: new { @class = "control-label col-md-2", style = "color: black;" })
                                                    <div class="col-md-10">

                                                        @Html.EditorFor(model => model.Cand_Pass, new { htmlAttributes = new { @class = "form-control", style = "color: darkblue;", @Value = "", placeholder = "Your password", required = "required", maxlength = "16", minlength = "6", onkeyup = "javascript:RemoveSpecialChar(this)" } })
                                                        @Html.ValidationMessageFor(model => model.Cand_Pass, "", new { @class = "text-danger" })
                                                    </div>
                                                </div>

                                                <div class="form-group" id="rstpass">
                                                    @Html.LabelFor(model => model.Cand_ResetPassCode, htmlAttributes: new { @class = "control-label col-md-2", style = "color: black;" })
                                                    <div class="col-md-10">
