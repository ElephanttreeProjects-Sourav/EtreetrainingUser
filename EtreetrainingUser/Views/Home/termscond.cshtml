                                    <h3>Login to your account</h3>
                                    <form action="~/Student/Cand_Login" method="post">
                                        @Html.AntiForgeryToken()

                                        @Html.EditorFor(model => model.Cand_EmailId, new { htmlAttributes = new { @class = "form-control", placeholder = "Email", required = "required", style = "text - align: left;", maxlength = "254", minlength = "4" } })
                                        @Html.ValidationMessageFor(model => model.Cand_EmailId, "", new { @class = "text-danger" })

                                        @Html.EditorFor(model => model.Cand_Pass, new { htmlAttributes = new { @class = "form-control", placeholder = "Password", required = "required", maxlength = "16", minlength = "6" } })
                                        @Html.ValidationMessageFor(model => model.Cand_Pass, "", new { @class = "text-danger" })

                                        <input type="submit" value="Login" style="background-color:lightseagreen">
                                    </form>

                                    <div class="cta">
                                        <a href="#" class="log" data-toggle="modal" onClick="PopUp('hide')" data-target="#myModal1">Forgot password?</a>
                                    </div>

                                </div>
                                <div class="form">
                                    <h3>Sign Up</h3>
                                    @using (Html.BeginForm("Cand_reg", "Student", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                    {
                                        @Html.AntiForgeryToken()

                                        @Html.EditorFor(model => model.CandName, new { htmlAttributes = new { @class = "form-control", required = "required", placeholder = "Name" } })
                                        @Html.ValidationMessageFor(model => model.CandName, "", new { @class = "text-danger" })

