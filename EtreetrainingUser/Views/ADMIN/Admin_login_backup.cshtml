                                        <label class="col-md-2 col-xs-4" style="text-align: left;">Email ID <span style="color:red">*</span> </label>
                                        <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
                                            @Html.EditorFor(model => model.Admin_EmailId, new { htmlAttributes = new { @class = "form-control", id = "emailid2", onmouseout = "mailcheck(this)" } })
